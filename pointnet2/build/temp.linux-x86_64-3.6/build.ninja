ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda/bin/nvcc

cflags = -pthread -DNDEBUG -g -fwrapv -O2 -Wall -g -fstack-protector-strong -Wformat -Werror=format-security -Wdate-time -D_FORTIFY_SOURCE=2 -fPIC -I/usr/local/lib/python3.6/dist-packages/torch/include -I/usr/local/lib/python3.6/dist-packages/torch/include/torch/csrc/api/include -I/usr/local/lib/python3.6/dist-packages/torch/include/TH -I/usr/local/lib/python3.6/dist-packages/torch/include/THC -I/usr/local/cuda/include -I/usr/include/python3.6m -c
post_cflags = -g -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=pointnet2_cuda -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++14
cuda_cflags = -I/usr/local/lib/python3.6/dist-packages/torch/include -I/usr/local/lib/python3.6/dist-packages/torch/include/torch/csrc/api/include -I/usr/local/lib/python3.6/dist-packages/torch/include/TH -I/usr/local/lib/python3.6/dist-packages/torch/include/THC -I/usr/local/cuda/include -I/usr/include/python3.6m -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -O2 -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=pointnet2_cuda -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_86,code=compute_86 -gencode=arch=compute_86,code=sm_86 -std=c++14
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags



build /data/translo_improve/pointnet2/build/temp.linux-x86_64-3.6/src/pointnet2_api.o: compile /data/translo_improve/pointnet2/src/pointnet2_api.cpp
build /data/translo_improve/pointnet2/build/temp.linux-x86_64-3.6/src/ball_query.o: compile /data/translo_improve/pointnet2/src/ball_query.cpp
build /data/translo_improve/pointnet2/build/temp.linux-x86_64-3.6/src/ball_query_gpu.o: cuda_compile /data/translo_improve/pointnet2/src/ball_query_gpu.cu
build /data/translo_improve/pointnet2/build/temp.linux-x86_64-3.6/src/group_points.o: compile /data/translo_improve/pointnet2/src/group_points.cpp
build /data/translo_improve/pointnet2/build/temp.linux-x86_64-3.6/src/group_points_gpu.o: cuda_compile /data/translo_improve/pointnet2/src/group_points_gpu.cu
build /data/translo_improve/pointnet2/build/temp.linux-x86_64-3.6/src/interpolate.o: compile /data/translo_improve/pointnet2/src/interpolate.cpp
build /data/translo_improve/pointnet2/build/temp.linux-x86_64-3.6/src/interpolate_gpu.o: cuda_compile /data/translo_improve/pointnet2/src/interpolate_gpu.cu
build /data/translo_improve/pointnet2/build/temp.linux-x86_64-3.6/src/sampling.o: compile /data/translo_improve/pointnet2/src/sampling.cpp
build /data/translo_improve/pointnet2/build/temp.linux-x86_64-3.6/src/sampling_gpu.o: cuda_compile /data/translo_improve/pointnet2/src/sampling_gpu.cu





